      <div class="container">

        <div id="content">

          {{ page.content }}

        </div><!-- /#content -->
      </div><!-- /.container -->
    </div><!-- /#primary-content -->

  </main>

  <!-- FOOTER -->
  <footer>
    <div class="footer-wrapper container padded-row content-large">

      <div id="footer-first">
        <div class="footer-first-wrapper">
          <div class="copyright-info">Owen T. S. Cooper | &#169; {{ "now"|date('Y') }} All rights reserved.</div>
        </div>
      </div><!-- /#footer-first -->

    </div><!-- /.container -->
