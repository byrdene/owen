
<div{{ attributes.addClass(
  'layout-container'
) }}>

  <!-- HEADER -->
  <header>
    <div id="site-header" class="container">
      <div class="content-large container padded-row">
        <a href="/rwanda"><span>Robotics & STEM Outreach in Rwanda</span></a>
      </div>

    </div><!-- /#site-header -->

  </header>

  <!-- MAIN PAGE CONTENT -->
  <main id="main-content" tabindex="-1">


    <!-- PRIMARY-CONTENT -->
